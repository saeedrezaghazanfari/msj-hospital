#: utils.py:93
msgid "مقدار فیلد را وارد کنید."
msgstr "Enter the field value."

#: utils.py:97
msgid "محتوای متن نباید شامل علامت باشد."
msgstr "Text content should not contain symbols."

#: validations.py:13
msgid "کدملی خود را وارد کنید."
msgstr "Enter your country code."

#: validations.py:15
msgid "الگوی کدملی شما صحیح نیست."
msgstr "Your national code template is not correct."

#: validations.py:18
msgid "کدملی شما یک بار در سیستم ثبت شده است."
msgstr "Your national code has been registered in the system once."

#: validations.py:25
msgid "محتوای فیلد را وارد کنید."
msgstr "Enter the content of the field."

#: validations.py:27
msgid "این فیلد باید بیشتر از 1 کاراکتر باشد."
msgstr "This field must be more than 1 character."

#: validations.py:29
msgid "این فیلد باید کمتر از 255 کاراکتر باشد."
msgstr "This field must be less than 255 characters."

#: validations.py:32
msgid "این فیلد باید شامل کاراکترهای غیر از اعداد باشد."
msgstr "This field must contain characters other than numbers."

#: validations.py:39
msgid "سن خود را وارد کنید"
msgstr "Enter your age"

#: validations.py:41
msgid "سن نباید منفی باشد."
msgstr "Age should not be negative."

#: validations.py:43
msgid "عدد سن معتبر نیست."
msgstr "The age number is not valid."

#: validations.py:50
msgid "شماره تلفن خود را وارد کنید."
msgstr "Enter your phone number."

#: validations.py:52
msgid "الگوی شماره تلفن شما صحیح نیست."
msgstr "Your phone number pattern is incorrect."

#: validations.py:59
msgid "الگوی ایمیل شما صحیح نیست."
msgstr "Your email template is incorrect."

#: validations.py:62
msgid "قبلا این آدرس ایمیل در سیستم ثبت شده است."
msgstr "This email address has already been registered in the system."

#: validations.py:71
msgid "فرمت فایل شما باید pdf باشد."
msgstr "Your file format must be pdf."

#: validations.py:75
msgid "فرمت فایل شما باید یکی از فرمت های mp4, webm, mkv باشد."
msgstr "Your file format must be one of mp4, webm, mkv formats."

#: validations.py:79
msgid "فرمت فایل شما باید یکی از فرمت های jpg, jpeg, png باشد."
msgstr "Your file format must be one of jpg, jpeg, png."

#: validations.py:83
msgid "فرمت فایل شما باید mp3 باشد."
msgstr "Your file format must be mp3."

#: validations.py:87
msgid "فرمت فایل شما باید یکی از فرمت های pdf, jpg, jpeg, png باشد."
msgstr "Your file format should be one of pdf, jpg, jpeg, png."
